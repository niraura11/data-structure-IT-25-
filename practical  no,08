#include <stdio.h>

int main() {
    int arr[50], size, i, j, temp;
    printf("Enter number of elements: ");
    scanf("%d", &size);

    printf("Enter %d elements:\n", size);
    for (i = 0; i < size; i++)
        scanf("%d", &arr[i]);

    for (i = 1; i < size; i++) {
        temp = arr[i];
        j = i - 1;
        while (j >= 0 && arr[j] > temp) {
            arr[j + 1] = arr[j];
            j--;
        }
        arr[j + 1] = temp;
    }

    printf("\nSorted array (Ascending Order): ");
    for (i = 0; i < size; i++)
        printf("%d ", arr[i]);
    printf("\n");

    return 0;
}
output:
Enter number of elements: 5
Enter 5 elements:
9 3 7 1 5

Sorted array (Ascending Order): 1 3 5 7 9
