#include <stdio.h>
#define MAX 10

int graph[MAX][MAX];
int visited[MAX];
int vertices;

void DFS_traverse(int node) {
    int i;
    printf("%d ", node);
    visited[node] = 1;

    for (i = 0; i < vertices; i++) {
        if (graph[node][i] == 1 && visited[i] == 0) {
            DFS_traverse(i);
        }
    }
}

int main() {
    int i, j, startNode;
    printf("Enter number of vertices: ");
    scanf("%d", &vertices);

    printf("Enter adjacency matrix:\n");
    for (i = 0; i < vertices; i++)
        for (j = 0; j < vertices; j++)
            scanf("%d", &graph[i][j]);

    printf("Enter starting vertex (0 to %d): ", vertices - 1);
    scanf("%d", &startNode);

    for (i = 0; i < vertices; i++) visited[i] = 0;

    printf("DFS Traversal: ");
    DFS_traverse(startNode);

    return 0;
}

