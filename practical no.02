#include <stdio.h>
#define MAX 5

int cq[MAX];
int start = -1;
int end = -1;

void enqueue(int value) {
    if ((start == 0 && end == MAX - 1) || (start == end + 1)) {
        printf("Queue full. Cannot add %d\n", value);
        return;
    }

    if (start == -1) {
        start = 0;
        end = 0;
    } else if (end == MAX - 1) {
        end = 0;
    } else {
        end++;
    }

    cq[end] = value;
    printf("%d inserted.\n", value);
}

void dequeue() {
    if (start == -1) {
        printf("Queue empty. Nothing to remove.\n");
        return;
    }

    printf("%d removed.\n", cq[start]);

    if (start == end) {
        start = -1;
        end = -1;
    } else if (start == MAX - 1) {
        start = 0;
    } else {
        start++;
    }
}

void display() {
    if (start == -1) {
        printf("Queue is empty.\n");
        return;
    }

    printf("Current queue: ");

    if (start <= end) {
        for (int i = start; i <= end; i++) {
            printf("%d ", cq[i]);
        }
    } else {
        for (int i = start; i < MAX; i++) {
            printf("%d ", cq[i]);
        }
        for (int i = 0; i <= end; i++) {
            printf("%d ", cq[i]);
        }
    }
    printf("\n");
}

int main() {
    int choice, val;

    while (1) {
        printf("\n--- Menu ---\n");
        printf("1. Add\n2. Remove\n3. View\n4. Exit\n");
        printf("Choose: ");
        scanf("%d", &choice);

        switch (choice) {
            case 1:
                printf("Enter value: ");
                scanf("%d", &val);
                enqueue(val);
                break;

            case 2:
                dequeue();
                break;

            case 3:
                display();
                break;

            case 4:
                printf("Program ended.\n");
                return 0;

            default:
                printf("Invalid option.\n");
        }
    }
}

