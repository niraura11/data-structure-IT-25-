#include <stdio.h>
#define MAX 10

int graph[MAX][MAX];
int visited[MAX];
int q[MAX];
int f = -1, r = -1;
int vertices;

void enqueue(int node) {
    if (r == MAX - 1) return;
    if (f == -1) f = 0;
    q[++r] = node;
}

int dequeue() {
    if (f == -1 || f > r) return -1;
    return q[f++];
}

void BFS_traversal(int start) {
    int i, current;
    printf("BFS Traversal: ");
    enqueue(start);
    visited[start] = 1;
    while ((current = dequeue()) != -1) {
        printf("%d ", current);
        for (i = 0; i < vertices; i++) {
            if (graph[current][i] == 1 && visited[i] == 0) {
                enqueue(i);
                visited[i] = 1;
            }
        }
    }
}

int main() {
    int i, j, startNode;
    printf("Enter number of vertices: ");
    scanf("%d", &vertices);
    printf("Enter adjacency matrix:\n");
    for (i = 0; i < vertices; i++)
        for (j = 0; j < vertices; j++)
            scanf("%d", &graph[i][j]);

    printf("Enter starting vertex (0 to %d): ", vertices - 1);
    scanf("%d", &startNode);

    for (i = 0; i < vertices; i++) visited[i] = 0;
    BFS_traversal(startNode);
    return 0;
}
